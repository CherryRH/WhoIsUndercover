
const tag: string = 'TestTag [WordProvider]:';

/*
 * 词语提供器
 * 随机输出卧底词或平民词
 * */
export default class WordProvider {
  static getRandomWord(): Word {
    // 随机选取
    const randomIndex = Math.floor(Math.random() * words.length);
    // 随机是否调换卧底词和平民词
    if (Math.random() > 0.5) {
      return new Word(words[randomIndex].undercoverWord, words[randomIndex].civilianWord);
    }
    else {
      return new Word(words[randomIndex].civilianWord, words[randomIndex].undercoverWord);
    }
  }
}

/*
 * 表示一对词语
 * */
export class Word {
  // 卧底词
  undercoverWord: string = '';
  // 平民词
  civilianWord: string = '';

  constructor(undercoverWord?: string, civilianWord?: string) {
    this.undercoverWord = undercoverWord ?? '';
    this.civilianWord = civilianWord ?? '';
  }

  public toString() {
    return '卧底词：' + this.undercoverWord + '  平民词：' + this.civilianWord;
  }
}

